RewriteBase /
RewriteCond %{REQUEST_FILENAME} !-f
RewriteCond %{REQUEST_FILENAME} !-d

RewriteRule ^(.*)$ /Cafeterias_Landing_public/$1
RewriteCond %{HTTP_HOST} ^(www.)?cafeteriasba.com$
